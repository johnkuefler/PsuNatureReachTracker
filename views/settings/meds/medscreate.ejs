<!DOCTYPE html>
<html>
<%- include('../../partials/header', {title: 'Nature Reach - Create Medications' }); %>

    <body>
        <% include ../../partials/navbar %>
            <div class="container">
                <br>
                <h1 class="ml-3">
                    <%= title %>
                </h1>
                <p class="ml-3">Create a new medication to add to the database</p>

                <form method="POST" action="/settings/meds/create">

                    <div class="form-group col-md-4 col-sm-12">
                        <label>Name of Medication</label>
                        <input required type="text" class="form-control" name="nameofmedication">
                    </div>

                    <div class="form-group col-md-4 col-sm-12">
                        <button type="submit" class="btn btn-primary">Submit</button>
                    </div>

                </form>

            </div>
            <% include ../../partials/scripts %>
    </body>

</html>