<!DOCTYPE html>
<html>
<%- include('../../partials/header', {title: 'Create Meds'}); %>

<body>
    <% include ../../partials/navbar %>
    <div class="container">
        <br>
        <h1><%= title %></h1>
        <p>Create Medication</p>
        <div class="container-fluid" style="width: 18rem;">
            <form method ="POST" action="/settings/meds/create">
                <div class="form-group" col-4>
                    <input required type="text" class="form-control" placeholder="Name of medication" name="nameofmedication">
                </div>
                <div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                </div>
            </form>
        </div>
    </div>
    <% include ../../partials/scripts %>
</body>

</html>